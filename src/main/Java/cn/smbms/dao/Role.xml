<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.smbms.dao">
    <insert id="save" parameterType="cn.smbms.pojo.Role">
        insert into smbms_role (
        roleCode,roleName,createdBy,cerationDate,modifyBy,modifyDate
        ) values (
        #{roleCode},#{roleName},#{createcBy},#{certionDate},
        #{modifyBy},#{modifyDate}
        )
    </insert>
    <delete id="fiaddelete" parameterType="int">
        delete smbms_role where id=#{id}
    </delete>
    <update id="fiadset" parameterType="cn.smbms.pojo.Role">
        update smbms_role set roleCode=#{roleCode},releName=#{releName},
        createdBy=#{createdBy},cerationDate=#{cerationDate},modifyBy=#{modifyBy},
        modifyDate=#{modifyDate} where id=#{id}
    </update>
    <select id="fiad" resultType="cn.smbms.pojo.Role">
        select * from smbms_role where id=#{id}
    </select>
    <select id="fiadlist" resultType="cn.smbms.pojo.Role">
        select * from smbms_role
    </select>
</mapper>